<div class="page-header">
  <%= link_to funcionarios_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_funcionario_path(@funcionario), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show funcionario</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nome:</dt>
  <dd><%= @funcionario.nome %></dd>

  <dt>Setor:</dt>
  <dd><%= @funcionario.setor %></dd>

  <dt>User:</dt>
  <dd><%= @funcionario.user %></dd>

</dl>
